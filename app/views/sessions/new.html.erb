<div>
  <p>
    <h1>Pi In The Sky Log In</h1>
  </p>
  <div>
    <%= render './errors' %>
  </div>
</div>
<div>
  <%= form_tag new_login_path do %>
    <%= label_tag :email %> <br>
    <%= email_field_tag :email %>
</div>
<div>
  <%= label_tag :password %> <br>
  <%= password_field_tag :password %>
</div>
<div>
  <%= submit_tag "Log In" %>
</div>

<% end %>

<%= link_to "Register", new_user_path %>